<!DOCTYPE html>
<html>
<head>
    <title></title>
    <%- include ../layout/head %>
</head>
<body>
    <%- include ../layout/nav %>

    <div class="panel panel-default">
        <div class="panel-heading">
            Users
        </div>
        <!-- /.panel-heading -->
        <div class="panel-body">
            <div class="table-responsive">
                <table class="table table-striped table-bordered table-hover">
                    <thead>
                        <tr>
                            <th>ID</th>
                            <th>Email</th>
                            <th>First Name</th>
                            <th>Last Name</th>
                            <th>Create Time</th>
                        </tr>
                    </thead>
                    <tbody>
                        <% if(data && data.users){ %>
                            <% for(var i = 0; i < data.users.length; i++){ %>
                                <tr>
                                    <td> <%= data.users[i]._id %> </td>
                                    <td> <%= data.users[i].email %> </td>
                                    <td> <%= data.users[i].firstname %> </td>
                                    <td> <%= data.users[i].lastname %> </td>
                                    <td>
                                        <%= data.users[i].created_at %>
                                    </td>
                                </tr>
                            <% } %>
                        <% } %>
                    </tbody>
                </table>
            </div>
            <!-- /.table-responsive -->
        </div>
        <!-- /.panel-body -->
    </div>
</body>
</html>










